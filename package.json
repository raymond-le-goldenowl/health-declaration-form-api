{
	"name": "health-declaration-form",
	"version": "1.0.0",
	"description": "",
	"main": "index.js",
	"scripts": {
		"start": "yarn run build & nodemon --delay 500ms --exec babel-node dist/index.js",
		"clean": "rm -rf dist",
		"build": "yarn clean && babel ./src --out-dir dist --copy-files",
		"test": "echo \"Error: no test specified\" && exit 1"
	},
	"keywords": [
		"nodejs",
		"express",
		"rest",
		"api",
		"sequelize",
		"mysql"
	],
	"author": "",
	"license": "ISC",
	"browserslist": {
		"production": [
			">0.2%",
			"not dead",
			"not op_mini all"
		],
		"development": [
			"last 1 chrome version",
			"last 1 firefox version",
			"last 1 safari version"
		]
	},
	"devDependencies": {
		"@babel/cli": "^7.17.6",
		"@babel/core": "^7.17.5",
		"@babel/eslint-parser": "^7.17.0",
		"@babel/node": "^7.16.8",
		"@babel/preset-env": "^7.16.11",
		"babel-plugin-import": "^1.13.3",
		"babel-plugin-module-resolver": "^4.1.0",
		"eslint-config-prettier": "^8.3.0",
		"eslint-plugin-prettier": "^4.0.0",
		"husky": "^4.3.8",
		"lint-staged": "^12.2.0",
		"nodemon": "^2.0.15",
		"prettier": "^2.5.1"
	},
	"husky": {
		"hooks": {
			"pre-commit": "lint-staged --allow-empty"
		}
	},
	"lint-staged": {
		"src/**/*.+(jsx|js|ts|tsx|md|json|yml|yaml)": [
			"prettier --write --ignore-unknown",
			"eslint --fix",
			"git add -A"
		]
	},
	"dependencies": {
		"body-parser": "^1.19.2",
		"cookie-parser": "^1.4.6",
		"cors": "^2.8.5",
		"dotenv": "^16.0.0",
		"express": "^4.17.3",
		"helmet": "^5.0.2",
		"moment": "^2.29.1",
		"mysql2": "^2.3.3",
		"sequelize": "^6.17.0",
		"validator": "^13.7.0",
		"vn-str": "^0.3.4"
	}
}
